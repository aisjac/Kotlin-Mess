<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid
                android:color="#004d98"/>
            <stroke
                android:color="#edbb00"
                android:width="1dp"/>
            <corners
                android:topLeftRadius="150dp"
                android:bottomLeftRadius="150dp"
                android:radius="10dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid
                android:color="#5390F9"/>
            <stroke
                android:color="#EFE7E7"
                android:width="1dp"/>
            <corners
                android:topLeftRadius="150dp"
                android:bottomLeftRadius="150dp"
                android:radius="10dp"/>
        </shape>
    </item>


</selector>